  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch Test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/debug/tests",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
    },
    {
      "name": "(gdb) Launch Cmd",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/debug/typedef",
      "args": ["--cpp_out=${workspaceRoot}/examples/examples/", "${workspaceRoot}/examples/simple_values.td"],
      "stopAtEntry": true,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
    }
  ]
}