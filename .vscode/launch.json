{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Launch Test",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceRoot}/build/debug/tests",
      "args": ["Simple template function."],
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "(lldb) Launch Cmd",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceRoot}/build/debug/typedef",
      "args": [
        "--cpp_out=/tmp",
        "${workspaceRoot}/examples/tests/templates.td"
      ],
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "(gdb) Launch Test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/debug/tests",
      "args": [
        "Struct with a symbol reference to another struct"
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
    },
    {
      "name": "(gdb) Launch Cmd",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/debug/typedef",
      "args": [
        "--cpp_out=${workspaceRoot}/tmp",
        "${workspaceRoot}/examples/tests/structs.td"
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
    }
  ]
}